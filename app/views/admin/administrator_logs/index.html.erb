<div class="portlet light">
  <div class="portlet-title">
    <div class="caption">
      <i class="icon-list"></i>登录日志
    </div>
    <div class="actions">
      <%= link_to url_for(params.permit!.merge(format: :xlsx)), class: 'btn btn-xs green', 'data-no-turbolink' => true do %>
          <i class="fa fa-download"></i>
          <span>导出表单</span>
      <% end %>
    </div>
  </div>
  <div class="portlet-body">
    <div class="dataTables_wrapper no-footer">
      <div class="row">
        <div class="col-md-12 col-sm-12">
          <%= search_form_for @search, url: admin_administrator_logs_path, class: 'form-inline' do |f| %>
          <div class="form-group">
            <%= f.label :administrator_nickname_cont, '管理员昵称' %>
            <%= f.text_field :administrator_nickname_cont, class: 'form-control gsh-input-share', placeholder: '管理员昵称' %>
          </div>
          <div class="form-group">
            <%= f.label :created_at_gteq, '登录时间' %>
            <%= f.text_field :created_at_gteq, class: 'form-control datepicker gsh-input-share-l', placeholder: '开始时间' %>
            <%= f.label :created_at_lteq, '-' %>
            <%= f.text_field :created_at_lteq, class: 'form-control datepicker gsh-input-share-r', placeholder: '结束时间'%>
          </div>
          <button type="submit" class="btn btn-default grey">搜索</button>
          <% end %>
        </div>
        <div class="col-md-6 col-sm-12">
          <div class="dataTables_filter pull-right">
          </div>
        </div>
      </div>
      <div class="table-scrollable">
        <table class="table table-striped table-bordered table-hover" id="sample_2">
          <thead>
            <tr>
              <th>
                管理员昵称
              </th>
              <th>
                IP
              </th>
              <th>
                登录时间
              </th>
            </tr>
          </thead>
          <tbody>
            <% @administrator_logs.each do |log| %>
            <tr>
              <td>
                <%= log.administrator.nickname %>
              </td>
              <td>
                <%= log.ip %>
              </td>
              <td>
                <%= l log.created_at %>
              </td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
      <div class="row">
        <div class="col-md-5 col-sm-12">
          <div class="dataTables_info" id="sample_4_info"><%= paginate_info(@administrator_logs) %></div>
        </div>
        <div class="col-md-7 col-sm-12">
          <%= paginate @administrator_logs, theme: 'admin' %>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- END EXAMPLE TABLE PORTLET-->
