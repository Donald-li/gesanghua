<div class="portlet light">
  <div class="portlet-title">
    <div class="caption">
      <i class="icon-list"></i><%= link_to @project.name, camp_projects_path %> >  探索营反馈
    </div>
    <div class="actions">
    <%= link_to new_camp_project_execute_feedback_path(@project), class: 'btn btn-xs blue', 'data-no-turbolink' => true do %>
          <i class="icon-plus"></i>
          <span>新增</span>
      <% end %>
    </div>
  </div>
  <div class="portlet-body">
    <div class="dataTables_wrapper no-footer">
      <div class="row">
        <div class="col-md-12 col-sm-12">
        </div>
        <div class="col-md-6 col-sm-12">
          <div class="dataTables_filter pull-right"></div>
        </div>
      </div>
      <div class="table-scrollable">
        <table class="table table-striped table-bordered table-hover" id="sample_2">
          <thead>
          <tr>
            <th>
              反馈时间
            </th>
            <th>
              反馈人
            </th>
            <th>
              操作
            </th>
          </tr>
          </thead>
          <tbody>
          <% @feedbacks.each do |feedback| %>
              <tr>
                <td>
                  <%= l feedback.created_at %>
                </td>
                <td>
                  <%= feedback.user.try(:name) %>
                </td>
                <td>
                  <%= link_to camp_project_execute_feedback_path(@project, feedback), 'data-no-turbolink' => true, class: 'btn btn-xs blue' do %>
                      <i class="fa fa-file"></i> 查看
                  <% end %>
                  <%= link_to edit_camp_project_execute_feedback_path(@project, feedback), 'data-no-turbolink' => true, class: 'btn btn-xs blue' do %>
                      <i class="fa fa-edit"></i> 修改
                  <% end %>
                </td>
              </tr>
          <% end %>
          </tbody>
        </table>
      </div>
      <div class="row">
        <div class="col-md-5 col-sm-12">
          <div class="dataTables_info" id="sample_4_info"><%= paginate_info(@feedbacks) %></div>
        </div>
        <div class="col-md-7 col-sm-12">
          <%= paginate @feedbacks, theme: 'admin' %>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- END EXAMPLE TABLE PORTLET-->
