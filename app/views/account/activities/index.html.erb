<script>
    //      左tab切换
    $(".person-body-left ul li").on("click", function () {
        $(".person-body-left ul li").removeClass("selectedli")
        $(this).addClass("selectedli")
        $(".leftLine").css("top", parseFloat($(this)[0].offsetTop) + "px")
        $.each($('.person-body-left ul li'), function (i, j) {
            $('.person-body-right > div').eq(i).css('display', 'none')
        });
        $('.person-body-right > div').eq($(this).index()).css('display', 'block')
    })
    //     右tab切换一级
    $('.donations li').on("click",function(){
        $(".donations li").removeClass("selectedDon")
        $(this).addClass("selectedDon")
        $.each($('.donations li'), function (i, j) {
            $('.donation').eq(i).css('display', 'none')
        });
        $('.donation').eq($(this).index()).css('display', 'block')
    })
    //     右tab切换二级
    $(".donations1 li").on("click", function () {
        $(".donations1 li").removeClass("selectedDo")
        $(this).addClass("selectedDo")
        $(".bottomline").css("left", parseFloat($(this)[0].offsetLeft) +31 +  "px")
    })
    if($('.donation .donation-cont').length===0){
        $('.donation-cont-no').css('display', 'block')
        $('.project-foot').css('display', 'none')
    }else{
        $('.donation-cont-no').css('display', 'none')
        $('.project-foot').css('display', 'block')
    }
    // 显示代码
    $('.show-code').on('click', function (event) {
        var $this = $(this);

        $this.next('pre').slideToggle(500);
    });

    // 模拟城市-无联动/无搜索
    var selector1 = $('#city-picker-selector').cityPicker({
        dataJson: cityData,
        renderMode: true,
        search: false,
        autoSelected: false,
        code: 'cityCode',
        level: 3
    });

    $('#ClickMe').click(function() {
        $('#code').center();
        $('#goodcover').show();
        $('#code').fadeIn();
    });
    $('#closebt').click(function() {
        $('#code').hide();
        $('#goodcover').hide();
    });
    $('.goodbut').click(function() {
        $('#code').hide();
        $('#goodcover').hide();
    });
    $('#delete').click(function() {
        $('#remove').center();
        $('#goodcover').show();
        $('#remove').fadeIn();
    });
    $('#remove-but').click(function() {
        $('#remove').hide();
        $('#goodcover').hide();
    });


</script>
